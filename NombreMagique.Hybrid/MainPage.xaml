<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:NombreMagique.Hybrid"
             xmlns:shared="clr-namespace:NombreMagique.Shared;assembly=NombreMagique.Shared"


             x:Class="NombreMagique.Hybrid.MainPage">

    <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type shared:Game}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>
